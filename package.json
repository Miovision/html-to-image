{
  "name": "html-to-image",
  "version": "0.0.2",
  "description": "Generates an image from a DOM node using HTML5 canvas.",
  "main": "./lib/index.js",
  "files": [
    "lib",
    "dist",
    "src"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "lint": "eslint --ext .js,.jsx src",
    "clean": "rimraf dist && rimraf lib",
    "build:lib": "cross-env NODE_ENV=test babel src --out-dir lib ",
    "build:dist": "webpack",
    "build": "npm run clean && npm run build:lib && npm run build:dist",
    "postinstall": "npm run build",
    "prebuild": "npm run lint",
    "prepublish": "npm run build",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bubkoo/html-to-image.git"
  },
  "keywords": [
    "screenshot",
    "cavas",
    "html",
    "dom",
    "render",
    "image",
    "vector",
    "svg"
  ],
  "author": "bubkoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bubkoo/html-to-image/issues"
  },
  "homepage": "https://github.com/bubkoo/html-to-image#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "imagediff": "^1.0.8",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack-preprocessor": "^1.0.1",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.10.0"
  }
}
